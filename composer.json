{
    "name": "flekssk/calculator-bundle",
    "description": "Slice the given string on numbers and operators and try to calculate",
    "license": "proprietary",
    "version": "1.0.1",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.14"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FKS\\StringCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FKS\\StringCalculator\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    }
}
